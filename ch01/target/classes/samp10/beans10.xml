<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="out" class="samp10.FileOutPut">
		<property name="fileName" value="t2.txt"></property>
	</bean>
	
	<bean id="vh" class="samp10.VehicleImpl">
		<property name="name" value="제니"></property>
		<property name="rider" value="버스"></property>
		<property name="out1" ref="out"></property>
	</bean>

</beans>
